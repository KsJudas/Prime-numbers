import java.util.Scanner;
public class task1 {
public static void main (String[] args){
	Scanner scan = new Scanner(System.in);
	System.out.print("Please write a number: ");
	int a = scan.nextInt();
	int tab[] = new int [a+1];
	System.out.println("The prime number are: ");
		for (int i=2; i<=a; i++){
		for	(int x=2; x<i; x++){ 	
		if (i%x == 0){tab[i] = i; break;}}
		int n=i-tab[i];
		if(n>0) {System.out.print(n +" ");}}}}

